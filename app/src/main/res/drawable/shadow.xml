<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#6ABBBBBB"/>
            <gradient
                android:angle="225"
                android:startColor="#68E91E63"
                android:endColor="#68E91E63"/>
            <corners android:radius="10dp" />
        </shape>
    </item>

    <item
        android:left="0dp"
        android:right="0dp"
        android:top="0dp"
        android:bottom="3dp">
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF"/>
            <corners android:radius="10dp" />
        </shape>
    </item>
</layer-list>